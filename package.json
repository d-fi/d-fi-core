{
  "name": "d-fi-core",
  "version": "1.3.2",
  "description": "Core module for d-fi",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint .",
    "prepare": "eslint . && tsc",
    "prebuild": "eslint .",
    "build": "tsc",
    "test": "ava"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": "https://github.com/d-fi/d-fi-core",
  "author": "Sayem Chowdhury",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.26.1",
    "browser-id3-writer": "^4.4.0",
    "delay": "^5.0.0",
    "node-html-parser": "^5.3.3",
    "p-queue": "^6.6.2",
    "spotify-uri": "^2.2.0",
    "spotify-web-api-node": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@types/spotify-web-api-node": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "ava": "^4.1.0",
    "eslint": "^8.11.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "!dist"
    ],
    "require": [
      "ts-node/register"
    ],
    "timeout": "2m",
    "verbose": true
  }
}
